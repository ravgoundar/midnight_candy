        "storage",
        "entity.name",
        "keyword.control.at-rule"
      ],
      "settings": {
        "foreground": "#10ccb3"
      }
    },
    {
      "name": "constant.numeric.css",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#ffffffcc"
      }
    }
